{
  "manifest_version": 2,
  "name": "Github-issues",
  "author": "Arnar",
  "version": "1.0.1",
  "description": "Overview of pulls and issues in Github repos",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "icon48.png"
  },
  "background": {
    "scripts": ["background/main.js"],
    "persistent": false
  },
  "permissions": [
    "storage",
    "alarms"
  ]
}
